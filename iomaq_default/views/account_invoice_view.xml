<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- agregar campos en Analisis de lineas de factura -->
        <record id="view_partner_property_form" model="ir.ui.view">
            <field name="name">IOMAQ: campos cost_unit y margin_unit</field>
            <field name="model">account.invoice.line.report</field>
            <field name="inherit_id" ref="l10n_ar_account.view_account_invoice_line_report_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='product_id']" position="after">
                    <field name="cost_unit"/>
                    <field name="margin_unit"/>
                </xpath>

            </field>
        </record>

    </data>
</openerp>
